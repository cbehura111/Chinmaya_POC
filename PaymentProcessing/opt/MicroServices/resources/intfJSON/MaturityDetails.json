{
    "apiURL": "${URL}",
    "servicetype": "HTTP",
    "methodType": "GET",
    "contentType": "application/json",
    "headers": {
        "Content-Type": "application/json",
        "Accept": "*/*",
        "Authorization": ""
    },
    "requestDef": {
        "vendor": "${REQUEST~apiRequest.requestObj.vendor}",
        "client": "${REQUEST~apiRequest.requestObj.client}",
        "requestId": "${REQUEST~apiRequest.requestObj.requestId}",
        "data": {
            "requestType": "${REQUEST~apiRequest.requestObj.data.requestType}",
            "requestId": "${REQUEST~apiRequest.requestObj.data.requestId}",
            "cif": "${REQUEST~apiRequest.requestObj.data.cif}",
            "typeOfDeposit": "${REQUEST~apiRequest.requestObj.data.typeOfDeposit}",
            "productCode": "${REQUEST~apiRequest.requestObj.data.productCode}",
            "period": "${REQUEST~apiRequest.requestObj.data.period}",
            "amount": "${REQUEST~apiRequest.requestObj.data.amount}",
            "custType": "${REQUEST~apiRequest.requestObj.data.custType}",
            "frequency": "${REQUEST~apiRequest.requestObj.data.frequency}"
        },
        "reqTimestamp": "${REQUEST~apiRequest.requestObj.reqTimestamp}",
        "chksum": "${REQUEST~apiRequest.requestObj.chksum}"
    },
    "responseDef": "",
    "logging": {
        "logRequired": "Y",
        "logType": "BOTH"
    },
    "timeout": "10"
}