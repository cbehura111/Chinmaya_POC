{
  "apiURL": "${URL}",
  "servicetype": "HTTP",
  "methodType": "POST",
  "contentType": "application/json",
  "headers": {
    "Content-Type": "application/json",
    "Accept": "*/*",
    "Authorization": ""
  },
  "requestDef": {
    "customerId": "${REQUEST~apiRequest.requestObj.customerId}",
    "branchName": "${REQUEST~apiRequest.requestObj.branchName}",
    "branchCode": "${REQUEST~apiRequest.requestObj.branchCode}",
    "transactionType": "${REQUEST~apiRequest.requestObj.transactionType}",
    "IFSC": "${REQUEST~apiRequest.requestObj.ifscCode}",
    "branchAddress": "${REQUEST~apiRequest.requestObj.branchAddress}"
  },
  "responseDef": "",
  "logging": {
    "logRequired": "Y",
    "logType": "BOTH"
  },
  "timeout":"10"
}
