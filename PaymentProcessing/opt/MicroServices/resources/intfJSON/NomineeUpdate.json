{
  "apiURL": "${URL}",
  "servicetype": "HTTP",
  "methodType": "POST",
  "contentType": "application/json",
  "headers": {
    "Content-Type": "application/json",
    "Accept": "*/*",
    "Authorization": ""
  },
  "requestDef": {
    "customerId": "${REQUEST~apiRequest.requestObj.customerId}",
    "accountNum": "${REQUEST~apiRequest.requestObj.debitAcctNo}",
    "name": "${REQUEST~apiRequest.requestObj.nomineeName}",
    "nomineeId": "${REQUEST~apiRequest.requestObj.nomineeId}",
    "relationship": "${REQUEST~apiRequest.requestObj.relationship}",
    "dob": "${REQUEST~apiRequest.requestObj.dob}",
    "address": "${REQUEST~apiRequest.requestObj.debitAccountHolderAddress}",
    "mobileNum": "${REQUEST~apiRequest.requestObj.mobileNumber}",
    "emailId": "${REQUEST~apiRequest.requestObj.email}",
    "city": "${REQUEST~apiRequest.requestObj.city}",
    "guardianAddress": "${REQUEST~apiRequest.requestObj.guardianAddress}",
    "guardianName": "${REQUEST~apiRequest.requestObj.guardianName}",
    "pinCode": "${REQUEST~apiRequest.requestObj.pinCode}",
    "sameDepositorAddress": "${REQUEST~apiRequest.requestObj.sameAddress}",
    "state": "${REQUEST~apiRequest.requestObj.state}",
    "claimPercent": "${REQUEST~apiRequest.requestObj.claimPercent}",
    "guardiandob": "${REQUEST~apiRequest.requestObj.guardiandob}",
    "guardianRelationship": "${REQUEST~apiRequest.requestObj.guardianRelationship}",
    "gender": "${REQUEST~apiRequest.requestObj.gender}"
  },
  "responseDef": "",
  "logging": {
    "logRequired": "Y",
    "logType": "BOTH"
  },
  "timeout":"10"
}
