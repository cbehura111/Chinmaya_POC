{
  "apiURL": "${URL}",
  "servicetype": "HTTP",
  "methodType": "POST",
  "contentType": "application/json",
  "headers": {
    "Content-Type": "application/json",
    "Accept": "*/*",
    "Authorization": ""
  },
  "requestDef": {
    "customerId": "${REQUEST~apiRequest.requestObj.customerId}",
    "personToMeet": "${REQUEST~apiRequest.requestObj.personToMeet}",
    "reason": "${REQUEST~apiRequest.requestObj.reason}",
    "transactionType": "${REQUEST~apiRequest.requestObj.transactionType}",
    "dateAndTime": "${REQUEST~apiRequest.requestObj.dateAndTime}"
  },
  "responseDef": "",
  "logging": {
    "logRequired": "Y",
    "logType": "BOTH"
  },
  "timeout":"10"
}
