{
  "apiURL": "${URL}",
  "servicetype": "HTTP",
  "methodType": "POST",
  "contentType": "application/json",
  "headers": {
    "Content-Type": "application/json",
    "Accept": "*/*",
    "Authorization": ""
  },
  "requestDef": {
    "cardNumber": "${REQUEST~apiRequest.requestObj.cardNumber}",
    "domAtmLimit": "${REQUEST~apiRequest.requestObj.domAtmLimit}",
    "domPosLimit": "${REQUEST~apiRequest.requestObj.domPosLimit}",
    "domEcomLimit": "${REQUEST~apiRequest.requestObj.domEcomLimit}",
    "domConlessLimit": "${REQUEST~apiRequest.requestObj.domConlessLimit}",
    "intlAtmLimit": "${REQUEST~apiRequest.requestObj.intlAtmLimit}",
    "intlPosLimit": "${REQUEST~apiRequest.requestObj.intlPosLimit}",
    "intlEcomLimit": "${REQUEST~apiRequest.requestObj.intlEcomLimit}",
    "intlConlessLimit": "${REQUEST~apiRequest.requestObj.intlConlessLimit}"
  },
  "responseDef": "",
  "logging": {
    "logRequired": "Y",
    "logType": "BOTH"
  },
  "timeout":"10"
}