{
  "apiURL": "${URL}",
  "servicetype": "HTTP",
  "methodType": "POST",
  "contentType": "application/json",
  "headers": {
    "Content-Type": "application/json",
    "Accept": "*/*",
    "Authorization": ""
  },
  "requestDef": {
    "customerId": "${REQUEST~apiRequest.requestObj.customerId}",
    "cardNumber": "${REQUEST~apiRequest.requestObj.cardNumber}",
    "mobileNo": "${REQUEST~apiRequest.requestObj.mobileNo}",
    "emailId": "${REQUEST~apiRequest.requestObj.emailId}",
    "communicationAddress": "${REQUEST~apiRequest.requestObj.communicationAddress}",
    "permanentAddress": "${REQUEST~apiRequest.requestObj.permanentAddress}"
  },
  "responseDef": "",
  "logging": {
    "logRequired": "Y",
    "logType": "BOTH"
  },
  "timeout":"10"
}