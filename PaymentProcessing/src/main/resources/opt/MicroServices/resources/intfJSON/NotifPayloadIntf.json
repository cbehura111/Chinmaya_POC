  "contentType": "application/json",
  "headers": {
    "Content-Type": "application/json",
    "Accept": "*/*",
    "Authorization": ""
  },
  "requestDef": {
    "apiRequest": {
      "requestObj": {
        "appId": "${REQUEST~apiRequest.requestObj.appId}",
        "notifPayload": "${REQUEST~apiRequest.requestObj.notifPayload}",
        "status": "${REQUEST~apiRequest.requestObj.status}",
        "sourceSystem": "${REQUEST~apiRequest.requestObj.sourceSystem}",
