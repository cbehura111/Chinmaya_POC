        "optdNotifTypes": "${REQUEST~apiRequest.requestObj.optdNotifTypes}",
        "txnStatus": "${REQUEST~apiRequest.requestObj.txnStatus}"
      }
    }
  },
  "responseDef": "",
  "logging": {
    "logRequired": "Y",
    "logType": "BOTH"
  },
  "timeout":"10"
}